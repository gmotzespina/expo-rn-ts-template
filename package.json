{
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "setup": "./scripts/run_setup.sh",
        "format": "./node_modules/.bin/prettier --write \"src/**/*.{ts,tsx,json}\"",
        "lint": "tslint --project .",
        "test:all": "run-s lint test:ci",
        "test:ci": "jest src/ --ci --coverage --maxWorkers 2",
        "start": "expo start",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "eject": "expo eject"
    },
    "dependencies": {
        "@react-navigation/native": "^5.0.5",
        "@react-navigation/stack": "^5.0.5",
        "i18n-js": "^3.5.1",
        "immer": "^9.0.6",
        "lodash": "4.17.14",
        "native-base": "^2.13.8",
        "expo": "~37.0.3",
        "react": "~16.9.0",
        "react-dom": "~16.9.0",
        "react-native": "https://github.com/expo/react-native/archive/sdk-37.0.1.tar.gz",
        "react-native-screens": "~2.2.0",
        "react-native-date-picker": "^2.7.9",
        "react-native-gesture-handler": "~1.5.0",
        "react-native-vector-icons": "^6.6.0",
        "react-native-web": "~0.11.7"
    },
    "devDependencies": {
        "@babel/core": "^7.8.6",
        "@types/react": "~16.9.23",
        "@types/react-native": "~0.61.17",
        "babel-preset-expo": "~8.1.0",
        "husky": "^4.2.5",
        "prettier": "^1.18.2",
        "react-test-renderer": "16.8.3",
        "tslint": "^5.13.0",
        "tslint-config-prettier": "^1.17.0",
        "tslint-react": "^3.6.0",
        "typescript": "~3.8.3"
    },
    "jest": {
        "preset": "@testing-library/react-native",
        "testEnvironment": "jsdom",
        "transformIgnorePatterns": [
            "node_modules/(?!(jest-)?react-native|react-navigation|@heap)"
        ],
        "transform": {
            "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
        },
        "coverageThreshold": {
            "global": {
                "branches": 24,
                "functions": 35,
                "lines": 61,
                "statements": 59
            }
        },
        "collectCoverageFrom": [
            "src/**/*.{js,jsx}",
            "utils/**/*.{js, jsx}",
            "!<rootDir>/node_modules/",
            "!**/fixtures/**.{js,jsx}",
            "!src/apolloClient/**.{js,jsx}"
        ],
        "coveragePathIgnorePatterns": [
            "package.json",
            "package-lock.json"
        ],
        "coverageReporters": [
            "json",
            "html"
        ],
        "setupFiles": [
            "./testenv.js"
        ],
        "setupFilesAfterEnv": [
            "@testing-library/react-native/cleanup-after-each"
        ],
        "globalSetup": "./jest-global-setup.js"
    },
    "private": true,
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    }
}
